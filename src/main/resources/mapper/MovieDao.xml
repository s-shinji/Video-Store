<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.sampleproject.dao.MovieDao">

    <insert id="insertMovie" parameterType="com.example.sampleproject.entity.Movie">
        INSERT INTO
            movie(movie, created, user_id)
        VALUES(
            #{movie.getMovie()},
            #{movie.getCreated()},
            #{movie.getUserId()})
    </insert>
<!-- INSERT INTO movie(movie, created) VALUES(?, ?) -->
</mapper>
